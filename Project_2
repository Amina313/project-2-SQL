        "# текст запроса\n",
        "query_6_5 = f'''\n",
        "SELECT \n",
        "    round(avg(length(key_skills)-length(replace(key_skills, CHR(9), ''))+1), 2) as result\n",
        "FROM public.VACANCIES v\n",
        "where \n",
        "(\n",
        "  name ilike '%data scientist%'or\n",

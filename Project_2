      "source": [
        "3. Найдите средние значения для нижней и верхней границы зарплатной вилки. Округлите значения до целого."
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 148,
      "id": "603cdbe4-0ec3-4106-8eaf-c73fcdd29165",
      "metadata": {
        "id": "603cdbe4-0ec3-4106-8eaf-c73fcdd29165"
      },
      "outputs": [],
      "source": [
        "# текст запроса\n",
        "query_4_3 = f'''SELECT\n",
        "      ROUND(AVG(v.salary_from)) mean_salary\n",
        "FROM \n",
        "       public.vacancies v\n",
        " WHERE v.salary_from is not Null\n",
        " UNION\n",
        "  SELECT\n",
        "        ROUND(AVG(v.salary_to))\n",
        "  FROM \n",
        "         public.vacancies v\n",
        "   WHERE v.salary_to is not Null\n",
        "   '''"
      ]
    },
